        "body": {
          "mode": "raw",
          "raw": "{\\n  \\\"name\\\": \\\"Test\\\",\\n  \\\"email\\\": \\\"test@example.com\\\",\\n  \\\"role\\\": \\\"Dev\\\",\\n  \\\"salary\\\": 30000\\n}"
        }
      }
    }
  ]
